generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("POSTGRES_URL")
}

model Subscriptions {
  id Int @id @default(autoincrement())
  contractId String
  keyXdr String
  protocol String?
  contractType String?
  storageType String?

  @@unique([contractId, keyXdr])
}